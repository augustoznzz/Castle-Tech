{
  "name": "castletech-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "kill-port 3002",
    "dev": "next dev -p 3002",
    "dev:3002": "next dev -p 3002",
    "dev:https": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:perf": "npm run build && npm run lighthouse",
    "lighthouse": "lighthouse http://localhost:3002 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless\"",
    "lighthouse:ci": "lighthouse-ci autorun",
    "perf:budget": "npm run build && npm run lighthouse:ci",
    "deploy": "npm run build && npm run export",
    "export": "next export"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "motion": "^12.23.22",
    "next": "^14.2.32",
    "nodemailer": "^7.0.6",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@netlify/plugin-nextjs": "5.13.2",
    "@next/bundle-analyzer": "^14.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "^14.2.32",
    "kill-port": "^2.0.1",
    "lighthouse": "^11.0.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
